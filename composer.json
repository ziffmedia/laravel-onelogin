{
    "name": "ziffdavis/laravel-onelogin",
    "description": "Simplified integration of OneLogin for Laravel applications.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ralph Schindler",
            "email": "ralph.schindler@ziffmedia.com"
        }
    ],
    "require": {
        "onelogin/php-saml": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "ZiffDavis\\Laravel\\Onelogin\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ZiffDavis\\Laravel\\Onelogin\\OneloginServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.8"
    }
}
